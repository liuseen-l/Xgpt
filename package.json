    "clsx": "^2.1.0",
    "express": "^4.19.2",
    "lint-staged": "^15.2.2",
    "parse-url": "^9.2.0",
    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.22.3",
